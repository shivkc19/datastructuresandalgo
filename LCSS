#include<iostream>
#include<cstring>
using namespace std;
int main()
{
    string a,b;
    cout<<"enter string :-"<<endl;
    cin>>a;

    cout<<"enter 2nd string:-";
    cin>>b;

    int s[a.size()+1][b.size()+1];
    int i,j;

for(i=0;i<a.size()+1;i++)
{
    s[i][0]=0;
}
for(j=0;j<b.size()+1;j++)
{
    s[0][j]=0;
}

    for(i=1;i<a.size()+1;i++)
    {
        for(j=1;j<b.size()+1;j++)
    {

        if(a[i-1]!=b[j-1])
        {
            s[i][j]=max(s[i][j-1],s[i-1][j]);
        }
        else
        {
            s[i][j]=s[i-1][j-1]+1;
        }
    }


    }
cout<<s[a.size()][b.size()];

return 0;
}
